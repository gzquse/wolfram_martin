Cell["Create a two-page form:", "Text",ExpressionUUID->"0f511897-f5cf-4992-9b8e-82c5626f6c52"],

Cell[BoxData[
 RowBox[{"DynamicModule", "[", "\[IndentingNewLine]", 
  RowBox[{
   RowBox[{"{", "date", "}"}], ",", "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{"FormFunction", "[", "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{"{", "\[IndentingNewLine]", 
       RowBox[{
        RowBox[{"{", 
         RowBox[{
          RowBox[{
           RowBox[{"{", 
            RowBox[{"\"\<title\>\"", ",", " ", "\"\<Title\>\""}], "}"}], " ", 
           "->", " ", 
           RowBox[{"{", 
            RowBox[{
            "\"\<Mr\>\"", ",", " ", "\"\<Mrs\>\"", ",", " ", "\"\<Ms\>\"", 
             ",", " ", "\"\<Dr\>\"", ",", " ", "\"\<Prof\>\""}], "}"}]}], ",",
           " ", 
          RowBox[{
           RowBox[{"{", 
            RowBox[{"\"\<firstName\>\"", ",", " ", "\"\<First Name\>\""}], 
            "}"}], " ", "->", " ", "String"}], ",", 
          RowBox[{
           RowBox[{"{", 
            RowBox[{"\"\<lastName\>\"", ",", " ", "\"\<Last Name\>\""}], 
            "}"}], " ", "->", " ", "String"}]}], "}"}], ",", 
        "\[IndentingNewLine]", 
        RowBox[{"{", 
         RowBox[{
          RowBox[{
           RowBox[{"{", 
            RowBox[{"\"\<DOB\>\"", ",", " ", "\"\<Date of Birth\>\""}], "}"}],
            " ", "->", " ", "\"\<ComputedDate\>\""}], ",", " ", 
          RowBox[{
           RowBox[{"{", 
            RowBox[{"\"\<email\>\"", ",", " ", "\"\<Email Address\>\""}], 
            "}"}], " ", "->", " ", "\"\<EmailAddress\>\""}]}], "}"}]}], 
       "\[IndentingNewLine]", "}"}], ",", "\[IndentingNewLine]", 
      RowBox[{
       RowBox[{"Text", "[", "\[IndentingNewLine]", 
        RowBox[{"StringForm", "[", "\[IndentingNewLine]", 
         RowBox[{
         "\"\<Welcome, `1` `2` `3`. We will send an email to `4` on your next \
