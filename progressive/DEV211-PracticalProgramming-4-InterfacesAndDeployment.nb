Cell[CellGroupData[{

Cell["Cloud Deployment", "Section",ExpressionUUID->"faa26757-4d29-40c7-b96b-71e652f1b23a"],

Cell[CellGroupData[{

Cell["APIFunction", "Subsection",ExpressionUUID->"52036ed6-dd10-4cd4-8cd7-88fe490793e8"],

Cell["\<\
APIs can be created with APIFunction, which has a very similar syntax to a \
single-page form:\
\>", "Text",ExpressionUUID->"b26273f0-eccf-418e-ab5b-72f0cf8937fd"],

Cell[BoxData[
 RowBox[{"FormFunction", "[", "\[IndentingNewLine]", 
  RowBox[{
   RowBox[{"{", "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{
      RowBox[{"{", 
       RowBox[{
        TagBox[
         FrameBox["\<\"Parameter 1 name\"\>"],
         "Placeholder"], ",", " ", 
        TagBox[
         FrameBox["\<\"Label\"\>"],
         "Placeholder"]}], "}"}], " ", "->", " ", 
      RowBox[{"<|", 
       RowBox[{
        RowBox[{"\"\<Interpreter\>\"", " ", "->", " ", 
         TagBox[
          FrameBox["\<\"Interpreter type\"\>"],
          "Placeholder"]}], ",", 
        TagBox[
         FrameBox["\<\"Further parameter options\"\>"],
         "Placeholder"]}], "|>"}]}], ",", "\[IndentingNewLine]", 
     TagBox[
      FrameBox["\<\"\[Ellipsis]\"\>"],
      "Placeholder"]}], "\[IndentingNewLine]", "}"}], ",", 
   "\[IndentingNewLine]", 
   TagBox[
    FrameBox["\<\"Function\"\>"],
    "Placeholder"], ",", "\[IndentingNewLine]", 
   TagBox[
    FrameBox["\<\"Format (e.g. \\\"JPEG\\\", \\\"String\\\", \\\"HTML\\\", \
etc.)\"\>"],
    "Placeholder"]}], "\[IndentingNewLine]", "]"}]], "Input",
 Editable->False,
 Evaluatable->False,ExpressionUUID->"05a35bfd-b192-4874-a28b-6f4b0f6df37d"],

Cell["Create an API to find the population of a town or city:", "Text",ExpressionUUID->"b6354feb-8422-4177-a8c8-45d3327532a1"],

Cell[BoxData[
 RowBox[{
  RowBox[{"popAPI", " ", "=", " ", 
   RowBox[{"APIFunction", "[", "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{"\"\<city\>\"", "->", 
       RowBox[{"\"\<ComputedCity\>\"", "->", 
        TemplateBox[{"\"Oxford\"", 
          RowBox[{"Entity", "[", 
            RowBox[{"\"City\"", ",", 
              RowBox[{"{", 
                
                RowBox[{
                 "\"Oxford\"", ",", "\"Oxfordshire\"", ",", 
                  "\"UnitedKingdom\""}], "}"}]}], "]"}], 
          "\"Entity[\\\"City\\\", {\\\"Oxford\\\", \\\"Oxfordshire\\\", \
\\\"UnitedKingdom\\\"}]\"", "\"city\""},
         "Entity"]}]}], "}"}], ",", "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{"\"\<The population of \>\"", "<>", " ", 
       RowBox[{"ToString", "[", 
        RowBox[{"#city", "[", "\"\<Name\>\"", "]"}], "]"}], "<>", 
       "\"\< is \>\"", "<>", 
       RowBox[{"ToString", "[", 
        RowBox[{"QuantityMagnitude", "[", 
         RowBox[{"#city", "[", "\"\<Population\>\"", "]"}], "]"}], "]"}]}], 
      "&"}]}], "\[IndentingNewLine]", "]"}]}], ";"}]], "Input",ExpressionUUID->\
"8cdc3cca-2e7c-4bca-aed9-d6aac5ca731d"],

Cell["Call it locally:", "Text",ExpressionUUID->"2f517ddb-c90b-4141-9331-3b6279f90285"],

Cell[BoxData[
 RowBox[{"popAPI", "[", 
  RowBox[{"<|", 
   RowBox[{"\"\<city\>\"", " ", "->", " ", "\"\<New York\>\""}], "|>"}], 
  "]"}]], "Input",ExpressionUUID->"ec2ff3fe-0b7e-4faa-8136-2c7da1d2ed70"],

Cell["Deploy the API to a URL:", "Text",ExpressionUUID->"05799f0f-3c17-4a86-af34-7b683338dcec"],

Cell[BoxData[
 RowBox[{"popAPIDeployed", " ", "=", " ", 
  RowBox[{"CloudDeploy", "[", "\[IndentingNewLine]", 
   RowBox[{
   "popAPI", ",", "\[IndentingNewLine]", 
    "\"\<PracticalProgramming/PopulationAPI\>\"", ",", "\[IndentingNewLine]", 
    
    RowBox[{"Permissions", "->", "\"\<Public\>\""}]}], "\[IndentingNewLine]", 
   "]"}]}]], "Input",ExpressionUUID->"a1b89f91-de24-4259-982f-792e11b3fc23"],

Cell["Call the deployed API:", "Text",ExpressionUUID->"fcb4a039-3fa3-465c-86be-012c566afbc4"],

Cell[BoxData[
 RowBox[{"URLExecute", "[", 
  RowBox[{"URLBuild", "[", 
   RowBox[{"popAPIDeployed", ",", " ", 
    RowBox[{"{", 
     RowBox[{"\"\<city\>\"", " ", "->", " ", "\"\<Chicago\>\""}], "}"}]}], 
   "]"}], "]"}]], "Input",ExpressionUUID->"870a4b6f-1ab8-45f8-95c0-\
dce443075cc1"],

Cell[TextData[{
 "The deployed API can also be used by appending ",
 StyleBox["?city=", "InlineCode"],
 StyleBox["location", "InlineCode",
  FontSlant->"Italic"],
 " to its URL."
}], "Text",ExpressionUUID->"ec8a74db-e784-44a4-98df-b4b195e09d4e"]
}, Open  ]]
}, Open  ]],

